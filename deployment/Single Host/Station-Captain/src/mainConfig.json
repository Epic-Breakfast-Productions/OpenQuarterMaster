{
	"system": {
		"hostname": "#{#mdnsHost}",
		"altHostnameIps": [],
		"systemDataDir": "/data",
		"dataDir": "#{system.systemDataDir}/oqm",
		"automaticContainerPruneFrequency": "monthly",
		"defaultUi": {
			"custom": false,
			"type": "core",
			"name": "base-station",
			"path": "/"
		}
	},
	"captain": {
		"demoMode": {
			"frequency": "hourly",
			"snapshotFile": ""
		}
	},
	"snapshots": {
		"location": "#{system.systemDataDir}/oqm-snapshots",
		"numToKeep": 5,
		"frequency": "weekly",
		"compressionAlg": "xz",
		"encryption": {
			"enabled": false,
			"pass": "<secret>"
		},
		"backup": {
			"method": "local",
			"enabled": false,
			"local": {
				"path": ""
			},
			"ssh": {
				"path": ""
			},
			"objStorage": {
				"accessKey": "<secret>",
				"secretKey": "<secret>",
				"bucket": "",
				"prefix": "",
				"endpointUrl": "",
				"region": "",
				"skipVerifySsl": false
			}
		}
	},
	"cert": {
		"externalDefault": "self",
		"additionalExternalSANs": [],
		"trustStore": {
			"systemExternalTrustStoreDir": "/etc/oqm/certs/trustStores",
			"files": {
				"selfSigned": {
					"crt": "#{cert.trustStore.systemExternalTrustStoreDir}/oqmSystemRootCert.crt",
					"p12": "#{cert.trustStore.systemExternalTrustStoreDir}/oqmSystemRootCert.p12",
					"p12Password": "<secret>"
				},
				"acme": {
					"crt": "#{cert.trustStore.systemExternalTrustStoreDir}/acmeTrustStore.crt"
				}
			}
		},
		"selfSigned": {
			"certs": {
				"CARootPrivateKey": "/etc/oqm/certs/oqmSystemCaRootPrivateKey.pem",
				"CARootCert":       "/etc/oqm/certs/oqmSystemCaRootCert.crt",
				"systemExternalSelfCertKey": "/etc/oqm/certs/oqmSystemExternalSelfCertKey.pem",
				"systemExternalSelfCert":    "/etc/oqm/certs/oqmSystemExternalSelfCert.crt",
				"systemExternalSelfCertCsr": "/etc/oqm/certs/oqmSystemExternalSelfCert.csr",
				"systemExternalKeystore": "/etc/oqm/certs/oqmSystemExternalKeystore.p12"
			},
			"rootCaTtl": 2920,
			"systemCertTtl": 365,
			"daysBeforeExpiryToRegen": 30,
			"certInfo": {
				"countryName": "US",
				"stateOrProvinceName": "PA",
				"localityName": "",
				"organizationName": "OQM-LOCAL",
				"organizationalUnitName": "",
				"caCommonName": "#{cert.selfSigned.certInfo.organizationName}"
			},
			"generateKeystore": false,
			"systemExternalKeystorePass": "<secret>",
			"internalKeystorePass": "<secret>"
		},
		"acme": {
			"contactEmail": "",
			"caServer": "",
			"eab": {
				"kid": "",
				"hmacEncoded": ""
			}
		},
		"provided": {
			"enabled": false,
			"cert": "",
			"key": ""
		}
	},
	"plugin": {
	}
}